    "debug": "^4.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.13.52",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "chai": "^4.3.6",
    "concurrently": "^7.2.2",
    "eslint": "^6.8.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "typescript": "^4.7.3"
